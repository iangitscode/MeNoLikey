{
  "manifest_version": 2,
  "name": "Me No Likey",
  "version": "0.1",
  "content_scripts":[
    {
    "matches": ["<all_urls>"],
    "css": ["removeLike.css"]
    }
  ],
  "icons":{
    "16":"16.png",
    "48":"48.png",
    "128":"128.png"
  },
  "background":{
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon": "16.png",
    "default_title": "Enable likes"
  },
  "permissions": [
    "activeTab"
  ]
}